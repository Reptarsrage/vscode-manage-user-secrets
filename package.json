{
	"name": "user-secrets",
	"displayName": "User Secrets",
	"description": "View and manage User Secrets in ASP .NET and .NET Core Apps",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.manageUserSecrets"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.manageUserSecrets",
				"title": "Manage User Secrets"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceExtname == .csproj",
					"command": "extension.manageUserSecrets",
					"group": "User Secrets"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	}
}
