{
  "name": "manage-user-secrets",
  "displayName": "Manage User Secrets",
  "description": "View and manage User Secrets in ASP .NET and .NET Core Apps",
  "repository": {
    "type": "git",
    "url": "https://github.com/Reptarsrage/vscode-manage-user-secrets.git"
  },
  "license": "MIT",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.manageUserSecrets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.manageUserSecrets",
        "title": "Manage User Secrets"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .csproj",
          "command": "extension.manageUserSecrets",
          "group": "User Secrets"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test",
    "lint": "tslint 'src/**/*.ts'",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "devDependencies": {
    "@types/mocha": "5.2.6",
    "@types/node": "10.14.4",
    "@types/sinon": "7.0.10",
    "prettier": "1.16.4",
    "sinon": "7.2.7",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3333",
    "vscode": "1.1.30"
  },
  "dependencies": {
    "@types/fs-extra": "^5.1.0",
    "fs-extra": "^7.0.1"
  }
}
